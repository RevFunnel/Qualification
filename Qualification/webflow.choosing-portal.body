<script>
$('.result-cta-cheveron').click(function() {
  var clicks = $(this).data('clicks');
  if (clicks) {
     $(this).removeClass('active');
  } else {
  $(this).addClass('active');
    
  }
  $(this).data("clicks", !clicks);
});
</script>

